#: ../../source/sql_manual/ddl/database/dropdatabse.md:5
msgid "DROP DATABASE -- Drop an existing database."
msgstr "drop database ：删除数据库"

#: ../../source/sql_manual/ddl/database/dropdatabse.md:7
msgid "Synopsis"
msgstr "命令格式"

#: ../../source/sql_manual/ddl/database/dropdatabse.md:12
